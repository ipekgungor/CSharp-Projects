<Window x:Class="GungorIpek.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GungorIpek"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid x:Name="Car_Services">
        <StackPanel>
            <Button x:Name="showAvailableServicesButton" Content="Show Available Services" HorizontalAlignment="Left" Margin="560,75,0,0" VerticalAlignment="Top" Height="45" Width="175" Click="showAvailableServicesButton_Click"/>
            <Button x:Name="backToMainButton" Content="Back to Main" HorizontalAlignment="Left" Margin="559,70,0,0" VerticalAlignment="Top" Height="45" Width="175" Click="backToMainButton_Click"/>
            <Button x:Name="closeButton" Content="Close" HorizontalAlignment="Left" Margin="560,80,0,0" VerticalAlignment="Top" Height="45" Width="175" Click="closeButton_Click"/>
        </StackPanel>
        <Frame x:Name="frame1" Content="Car Services" HorizontalAlignment="Left" Margin="52,76,0,50" Width="456"/>
    </Grid>
</Window>
